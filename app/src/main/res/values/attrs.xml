<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EditSliderView">
        <attr name="state" format="enum">
            <enum name="all" value="0" />
            <enum name="onlySlider" value="1" />
            <enum name="withoutSlider" value="2" />
            <enum name="withoutCancel" value="3" />
            <enum name="withoutApply" value="4" />
        </attr>

        <attr name="valueFrom" format="float"/>
        <attr name="valueTo" format="float"/>
        <attr name="value" format="float"/>
    </declare-styleable>
</resources>